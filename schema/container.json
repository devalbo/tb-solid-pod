{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      ]
    },
    "http://purl.org/dc/terms/title": {
      "type": "string"
    },
    "http://purl.org/dc/terms/description": {
      "type": "string"
    },
    "http://purl.org/dc/terms/created": {
      "type": "string"
    },
    "http://purl.org/dc/terms/modified": {
      "type": "string"
    },
    "http://www.w3.org/ns/ldp#contains": {
      "anyOf": [
        {
          "type": "object",
          "properties": {
            "@id": {
              "type": "string",
              "format": "uri"
            }
          },
          "required": [
            "@id"
          ],
          "additionalProperties": false
        },
        {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "@id": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "@id"
            ],
            "additionalProperties": false
          }
        }
      ]
    }
  },
  "required": [
    "@type"
  ],
  "additionalProperties": {}
}