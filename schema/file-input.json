{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "mimeType": {
      "type": "string"
    },
    "size": {
      "type": "integer",
      "minimum": 0,
      "maximum": 9007199254740991
    },
    "description": {
      "type": "string"
    },
    "authorId": {
      "type": "string",
      "format": "uri"
    },
    "isImage": {
      "type": "boolean"
    },
    "width": {
      "type": "integer",
      "exclusiveMinimum": 0,
      "maximum": 9007199254740991
    },
    "height": {
      "type": "integer",
      "exclusiveMinimum": 0,
      "maximum": 9007199254740991
    },
    "location": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "mimeType",
    "size"
  ],
  "additionalProperties": false
}