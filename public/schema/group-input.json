{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "string",
      "enum": [
        "organization",
        "team",
        "group"
      ]
    },
    "description": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "logo": {
      "type": "string",
      "format": "uri"
    },
    "parentId": {
      "type": "string",
      "format": "uri"
    },
    "memberIds": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      }
    }
  },
  "required": [
    "name",
    "type"
  ],
  "additionalProperties": false
}