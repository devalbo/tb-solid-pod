{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "type": "string",
      "const": "http://www.w3.org/ns/solid/terms#TypeRegistration"
    },
    "http://www.w3.org/ns/solid/terms#forClass": {
      "type": "object",
      "properties": {
        "@id": {
          "type": "string",
          "format": "uri"
        }
      },
